\section{Question 2}
\begin{equation}
\tilde{a}=2i+3j+6k \\
\end{equation}
\begin{equation}
\tilde{b}=3i+5j+7k \\
\end{equation}
\begin{equation}
\tilde{c}=4i+2j+k \\
\end{equation}
\begin{equation}
\tilde{a}\times\tilde{b}=\[ \left| \begin{array}{ccc}
i & j & k \\
2 & 3 & 6 \\
3 & 5 & 7 \end{array} \right|\] 
\end{equation}

